CREATE TABLE &quot;Brands&quot; (
    &quot;id&quot;    INTEGER NOT NULL UNIQUE,
    &quot;name&quot;  TEXT NOT NULL UNIQUE, -- e.g., &quot;Rolex&quot;
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT)
);

-- Table to store the straps
CREATE TABLE &quot;Straps&quot; (
    &quot;id&quot;          INTEGER NOT NULL UNIQUE,
    &quot;name&quot;        TEXT NOT NULL,    -- e.g., &quot;Oysterflex Rubber&quot;
    &quot;description&quot; TEXT,             -- e.g., &quot;Durable and waterproof&quot;
    &quot;material&quot;    TEXT,             -- e.g., &quot;Rubber&quot;
    &quot;price&quot;       REAL,             -- e.g., 250.00
    &quot;brand_id&quot;    INTEGER NOT NULL, -- This links the strap to a brand
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT),
    FOREIGN KEY(&quot;brand_id&quot;) REFERENCES &quot;Brands&quot;(&quot;id&quot;)
);</sql><sql name="SQL 2*">-- Add some brands to the Brands table
